{
  "routes": [
    {
      "route": "/api/avd-start",
      "allowedRoles": ["AVD Users", "AVD Administrators"]
    },
    {
      "route": "/api/avd-status",
      "allowedRoles": ["AVD Users", "AVD Administrators"]
    },
    {
      "route": "/api/avd-stop",
      "allowedRoles": ["AVD Administrators"]
    }
  ],
  "responseOverrides": {
    "401": {
      "redirect": "/unauthorized",
      "statusCode": 302
    },
    "403": {
      "redirect": "/unauthorized",
      "statusCode": 302
    }
  },
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*", "/css/*", "/js/*", "/favicon.ico"]
  }
}
