{
  "routes": [
    { "route": "/login",
      "redirect": "/.auth/login/aad?post_login_redirect_url=/" },
    { "route": "/logout",
      "redirect": "/.auth/logout?post_logout_redirect_url=/" },
    { "route": "/api/avd/*",
      "allowedRoles": ["authenticated"] },
    { "route": "/",
      "rewrite": "/index.html",
      "allowedRoles": ["authenticated"] }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [ "/.auth/*", "/api/*" ]
  }
}
